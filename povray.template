commandline = povray +I${InputFile} +W${width} +H${height} +FN +A
= povray template =
-
[jobname]
type = Jobname
defaultValue = povray
title = Jobname
size = 550x80
-
[InputFile]
type = SingleInputFile
size = 550x80
title = Input File
filter = BasenameFilter

-
[width]
