commandline = beast ${file}
application = BEAST
cpus = 1
force_single = true

= Create a BEAST job = 
-
[file]
type = SingleInputFile
title = BEAST XML file
size = 2000x70
filter = BasenameFilter
help = you need to specify a BEAST XML input file
- 
[ApplicationVersionSelector]
type = ApplicationVersionSelector
size = 350x70
title = Application version

[walltime]
type = Walltime
title = Walltime
defaultAmount = 1
defaultUnit = hours
#useHistory = false
size = 200x70
-
[QueueSelector]
title = Queue
type = QueueSelector
size = 1000x70
-
[jobname]
type = Jobname
defaultValue = beast_job
title = Jobname
size = 2000x70
-
[email]
type = Email
size = 2000x120
